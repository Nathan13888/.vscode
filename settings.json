  "C_Cpp.errorSquiggles": "Enabled",
  "files.exclude": {
    "**/*.exe": true,
    "node_modules/":true,
  },
  "code-runner.enableAppInsights": false,
  "code-runner.runInTerminal": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.showExecutionMessage": true,
  "git.autofetch": true,
  "code-runner.defaultLanguage": "cpp",
  "code-runner.showRunIconInEditorTitleMenu": false,
  "code-runner.customCommand": "",
  "code-runner.executorMapByFileExtension": {},
  "code-runner.executorMap": {
    "cpp": "cd $dir && g++ $fileName -std=c++11 -Wshadow -Wall -o $fileNameWithoutExt -O2 -Wno-unused-result && /usr/bin/time -f /usr/bin/time -f \"\\n\\n[Real] %es\\n[User] %Us\\n[Syst] %Ss\\n[Stck] %pKB\\n[Exit] %x\" $dir$fileNameWithoutExt"
  },
  "editor.tokenColorCustomizations": {
    "functions": {
      "foreground": "#cdff1a",
      "fontStyle": "italic"
    },
    "types": {
      "foreground": "#ffc13a",
      "fontStyle": "bold"
    },
    "semanticHighlighting": true,
    "numbers": "#fcf816",
    "comments": "#555555",
    // "strings": "#FF0000",
    // "keywords": "#cdff1a",
    "textMateRules": [
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#34fcb9"
        }
      },
      {
        "scope": "entity.name.type",
        "settings": {
          "foreground": "#1cf1d5",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "entity.name.type.class.reference",
        "settings": {
          "foreground": "#58c09d"
        }
      },
      {
        "scope": "variable.parameter",
        "settings": {
          "foreground": "#00d9ff",
          "fontStyle": "italic bold"
        }
      },
      {
        "scope": "variable.other.property.cli",
        "settings": {
          "foreground": "#1d7e8f",
          "fontStyle": "italic bold"
        }
      },
      // {
      //   "scope": "variable.other.local",
      //   "settings": {
      //     "foreground": "#FF0000",
      //     "fontStyle": "bold"
      //   }
      // },
      // {
